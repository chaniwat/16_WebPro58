<?xml version="1.0" encoding="ISO-8859-1"?>

<taglib xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
        version="2.1">

    <tlib-version>1.0</tlib-version>
    <short-name>Template Tag lib</short-name>
    <tag>
        <name>page</name>
        <tag-class>com.alumnisystem.page.Page</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <name>title</name>
        </attribute>
    </tag>
    <tag>
        <name>alumniProfile</name>
        <tag-class>com.alumnisystem.page.profile.AlumniProfile</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>user</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>alumniTrack</name>
        <tag-class>com.alumnisystem.page.profile.AlumniTrack</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>user</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>staffProfile</name>
        <tag-class>com.alumnisystem.page.profile.StaffProfile</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>user</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>teacherProfile</name>
        <tag-class>com.alumnisystem.page.profile.TeacherProfile</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>user</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>footerpage</name>
        <tag-class>com.alumnisystem.page.FooterPage</tag-class>
        <body-content>empty</body-content>
    </tag>
    <tag>
        <name>pageadmin</name>
        <tag-class>com.alumnisystem.page.PageAdmin</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <name>title</name>
        </attribute>
        <attribute>
            <name>skipBootstrapJsLoad</name>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>


    <!-- Invoke 'Generate' action to add tags or functions -->

</taglib>
